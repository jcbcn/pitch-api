[
    {
      'repeat(0, 100)': {
      "id": "{{guid()}}",
      "shortName": "{{surname()}}",
      name() { return `${firstName(["male"])} ${this.shortName}`; },
      "nationality": "{{country()}}",
      "positions": [
          {
            'repeat(2)': '{{random("GK", "LB", "CB", "RB", "LM", "CM", "RM", "ST")}}'
          }
      ],
      "rating": '{{integer(50, 99)}}',
      "form": '{{floating(0, 1, 2)}}'
      }
    }
  ]